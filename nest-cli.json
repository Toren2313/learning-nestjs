{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-gateway",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "app-config": {
      "type": "library",
      "root": "libs/app-config",
      "entryFile": "index",
      "sourceRoot": "libs/app-config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/app-config/tsconfig.lib.json"
      }
    },
    "contracts": {
      "type": "library",
      "root": "libs/contracts",
      "entryFile": "index",
      "sourceRoot": "libs/contracts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/contracts/tsconfig.lib.json"
      }
    },
    "prices-microservice": {
      "type": "application",
      "root": "apps/prices-microservice",
      "entryFile": "main",
      "sourceRoot": "apps/prices-microservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/prices-microservice/tsconfig.app.json"
      }
    },
    "twelve-data": {
      "type": "library",
      "root": "libs/twelve-data",
      "entryFile": "index",
      "sourceRoot": "libs/twelve-data/src",
      "compilerOptions": {
        "tsConfigPath": "libs/twelve-data/tsconfig.lib.json"
      }
    }
  }
}